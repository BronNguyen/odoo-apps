<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <!-- THINH the DatetimeFilterItem need to be inserted into the origin ControlPanel (js file) to be available here-->
    <t t-name="web_datetime_panel.ControlPanel" t-inherit="web.ControlPanel" t-inherit-mode="extension" owl="1">
        <xpath expr="//div[@t-ref='root']" position="inside">
            <DatetimeFilterItem class="o_filter_menu" />
        </xpath>
    </t>
</templates>
